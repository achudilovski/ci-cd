[Unit]
Description=ecosystem
Requires=docker.service
After=network.target docker.service
Wants=network.target


[Service]
ExecStartPre=/data/docker/pre-startup-service.sh
ExecStart=/data/docker/startup-service.sh
ExecStop=/data/docker/stop-service.sh
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target
